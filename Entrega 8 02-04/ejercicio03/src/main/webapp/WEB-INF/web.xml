<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>InicioServlet</servlet-name>
		<display-name>InicioServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.servlet.InicioServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IniciarSesionServlet</servlet-name>
		<display-name>IniciarSesionServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.servlet.IniciarSesionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>prueba</servlet-name>
		<display-name>prueba</display-name>
		<description></description>
		<servlet-class>com.educacionit.servlet.prueba</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LimpiarSessionServlet</servlet-name>
		<display-name>LimpiarSessionServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.servlet.LimpiarSessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
  		<servlet-name>InicioServlet</servlet-name>
  		<url-pattern>/InicioServlet</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  		<servlet-name>IniciarSesionServlet</servlet-name>
  		<url-pattern>/IniciarSesionServlet</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  		<servlet-name>prueba</servlet-name>
  		<url-pattern>/prueba</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
  		<servlet-name>LimpiarSessionServlet</servlet-name>
  		<url-pattern>/LimpiarSessionServlet</url-pattern>
  	</servlet-mapping>
</web-app>
